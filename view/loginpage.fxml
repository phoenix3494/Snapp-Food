            <Font name="System Bold" size="38.0" />
         </font>
      </Label>
      <PasswordField fx:id="passwordTXF" layoutX="267.0" layoutY="288.0" prefHeight="37.0" prefWidth="267.0" promptText="Password" style="-fx-background-radius: 20;">
         <font>
            <Font size="15.0" />
         </font>
      </PasswordField>
      <TextField fx:id="usernameTXF" layoutX="267.0" layoutY="195.0" prefHeight="37.0" prefWidth="267.0" promptText="Username(Email)" style="-fx-background-radius: 20;">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <Button layoutX="267.0" layoutY="412.0" mnemonicParsing="false" onAction="#login" prefHeight="59.0" prefWidth="267.0" style="-fx-background-color: #19f70d; -fx-background-radius: 20;" text="Login" textFill="#41465e">
