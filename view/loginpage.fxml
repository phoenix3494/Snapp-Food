            <Font name="System Bold" size="24.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Label fx:id="errorLBL" alignment="CENTER" layoutX="165.0" layoutY="360.0" prefHeight="31.0" prefWidth="469.0" textFill="#fc0505">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
