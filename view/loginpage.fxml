<AnchorPane prefHeight="650.0" prefWidth="800.0" style="-fx-background-color: #1a0439;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginPageController">
   <children>
      <Label alignment="CENTER" layoutX="321.0" layoutY="27.0" prefHeight="71.0" prefWidth="158.0" text="Login" textFill="#ebff56">
